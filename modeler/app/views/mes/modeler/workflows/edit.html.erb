<% content_for :page_title do %>
  <%= Mes.t(:editing_resource, resource: Mes::Workflow.model_name.human) %>
<% end %>

<%= render :partial => 'mes/modeler/shared/error_messages', :locals => { :target => @workflow } %>

<%= form_for [:modeler, @workflow] do |f| %>
  <fieldset>
    <%= render :partial => 'form', :locals => { f: f, workflow: @workflow } %>
    <%= render :partial => 'mes/modeler/shared/edit_resource_links' %>
  </fieldset>
<% end %>
